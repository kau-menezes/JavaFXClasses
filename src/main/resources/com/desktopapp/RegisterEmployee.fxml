<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="488.0" prefWidth="770.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.desktopapp.RegisterEmployeeController">
  <children>
    <MenuBar prefHeight="24.0" prefWidth="770.0" VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="New" />
            <MenuItem mnemonicParsing="false" text="Open…" />
            <Menu mnemonicParsing="false" text="Open Recent" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Close" />
            <MenuItem mnemonicParsing="false" text="Save" />
            <MenuItem mnemonicParsing="false" text="Save As…" />
            <MenuItem mnemonicParsing="false" text="Revert" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Preferences…" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
          <items>
            <MenuItem mnemonicParsing="false" text="Undo" />
            <MenuItem mnemonicParsing="false" text="Redo" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Cut" />
            <MenuItem mnemonicParsing="false" text="Copy" />
            <MenuItem mnemonicParsing="false" text="Paste" />
            <MenuItem mnemonicParsing="false" text="Delete" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Select All" />
            <MenuItem mnemonicParsing="false" text="Unselect All" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Pane prefHeight="48.0" prefWidth="770.0" style="-fx-background-color: #8c8cf2;">
               <children>
                  <Button fx:id="homePageButton" layoutX="14.0" layoutY="12.0" mnemonicParsing="false" onMouseClicked="#homeNav" prefHeight="25.0" prefWidth="91.0" style="-fx-background-color: #5353ec;" text="Products" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Button>
                  <Button fx:id="registerPageButton" layoutX="127.0" layoutY="12.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="91.0" style="-fx-background-color: #5353ec;" text="Employees" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Button>
                  <Button fx:id="logOutButton" layoutX="665.0" layoutY="11.0" mnemonicParsing="false" onMouseClicked="#onButtonClick" prefHeight="27.0" prefWidth="91.0" style="-fx-background-color: #FFFFFF; -fx-border-color: #5353ec; -fx-border-radius: 4;" text="Log Out" textFill="#5353ec">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Button>
               </children>
            </Pane>
            <Button fx:id="registerButton" layoutX="311.0" layoutY="315.0" mnemonicParsing="false" onMouseClicked="#register" prefHeight="25.0" prefWidth="91.0" style="-fx-background-color: #5353ec;" text="Register" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Text fill="#5353ec" layoutX="175.0" layoutY="110.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Register a new employee:">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Text>
            <Label layoutX="175.0" layoutY="155.0" text="Login">
               <font>
                  <Font name="Bosch Sans Bold" size="12.0" />
               </font>
            </Label>
            <Label layoutX="174.0" layoutY="253.0" text="Password">
               <font>
                  <Font name="Bosch Sans Bold" size="12.0" />
               </font>
            </Label>
            <TextField fx:id="emailInput" layoutX="275.0" layoutY="151.0" prefHeight="25.0" prefWidth="217.0" promptText="Type its email..." />
            <PasswordField fx:id="passwordInput" layoutX="277.0" layoutY="249.0" prefHeight="25.0" prefWidth="217.0" promptText="Type its password..." />
            <Label layoutX="174.0" layoutY="202.0" text="Name">
               <font>
                  <Font name="Bosch Sans Bold" size="12.0" />
               </font>
            </Label>
            <TextField fx:id="nameInput" layoutX="277.0" layoutY="198.0" prefHeight="25.0" prefWidth="217.0" promptText="Type its name..." />
      </children>
    </AnchorPane>
  </children>
</VBox>
